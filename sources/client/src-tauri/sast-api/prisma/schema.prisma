datasource db {
  provider = "sqlite"
  url      = "file:../../dev.sqlite"
}

generator client {
  provider = "cargo prisma"
  output   = "../src/prisma.rs"
}

model Project {
  id        String   @id @default(uuid())
  name      String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
