{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "sast",
  "version": "0.0.0",
  "identifier": "de.leonsnajdr.sast",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "yarn dev:nuxt",
    "beforeBuildCommand": "yarn build:nuxt"
  },
  "app": {
    "windows": [
      {
        "decorations": false,
        "title": "Sast",
        "minWidth": 800,
        "minHeight": 600,
        "resizable": true,
        "fullscreen": false,
        "dragDropEnabled": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": ["icons/32x32.png", "icons/128x128.png", "icons/128x128@2x.png", "icons/icon.icns", "icons/icon.ico"],
    "createUpdaterArtifacts": true
  },
  "plugins": {
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEY2MkNDRkYwMTIxRTNBREIKUldUYk9oNFM4TThzOXNSQXJZOHZiZXZ3RFI4UnRWRFpibGhTK2xabjl6NjZQb1FGcFU3aVRUdWMK",
      "endpoints": ["https://github.com/LeonSnajdr/sast/releases/latest/download/latest.json"]
    }
  }
}
